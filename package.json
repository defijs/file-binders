{
  "name": "matreshka-binders-file",
  "private": true,
  "version": "0.0.0-no-versioning",
  "scripts": {
    "test": "npm run unit && npm run lint",
    "unit": "babel-node node_modules/.bin/babel-istanbul cover test/index.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "check-updates": "ncu",
    "upgrade": "npm run check-updates -- --upgradeAll && npm install",
    "lint": "eslint ./test ./src",
    "compile-npm": "babel src -d npm",
    "bundle": "webpack",
    "copy-readme": "ncp ./README.md ./npm/README.md",
    "release": "npm test && rimraf npm/*.js npm/README.md bundle/* && npm run copy-readme && npm run compile-npm && npm run bundle && cd npm && npm publish && cd .. && git commit -m 'Publishing ${npm_package_version}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matreshka/matreshka-binders-file.git"
  },
  "author": "Andrey Gubanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matreshka/matreshka-binders-file/issues"
  },
  "homepage": "https://github.com/matreshka/matreshka-binders-file#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-check-es2015-constants": "^6.8.0",
    "babel-plugin-transform-es2015-arrow-functions": "^6.8.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.15.0",
    "babel-plugin-transform-es2015-computed-properties": "^6.8.0",
    "babel-plugin-transform-es2015-destructuring": "^6.9.0",
    "babel-plugin-transform-es2015-modules-simple-commonjs": "^0.3.0",
    "babel-plugin-transform-es2015-parameters": "^6.11.4",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.8.0",
    "babel-plugin-transform-es2015-template-literals": "^6.8.0",
    "babel-plugin-transform-object-spread-inline": "0.0.2",
    "coveralls": "^2.11.12",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "jasmine": "^2.5.1",
    "jsdom": "^9.5.0",
    "makeelement": "0.0.1",
    "matreshka": "^2.0.0-prealpha.17",
    "ncp": "^2.0.0",
    "npm-check-updates": "^2.8.0",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.2"
  }
}
